  "content_context": [
    {
      "order": 0,
      "type": "text",
      "text": "<p>Gerrit van Bakel ontwierp in 1982 de Tarim Machine, een bewegend kunstwerk. Je ziet het kunstwerk en daaronder een houten model.</p>"
    },
