  "question": {
    "text": "<p>Bereken exact de afstand tussen <math xmlns=\"http://www.w3.org/1998/Math/MathML\"><mi>R</mi>&#160;</math>en <math xmlns=\"http://www.w3.org/1998/Math/MathML\"><mi>S</mi>&#160;</math>.</p>",
    "max_score": 4,
    "is_self_scored": false
  },
