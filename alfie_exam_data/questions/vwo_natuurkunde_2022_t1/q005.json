    {
      "order": 1,
      "type": "image",
      "media_path": "media/in-exam/img_3b0ff7d736c4.png",
      "azure_url": "https://bbprodpublicsa.blob.core.windows.net/public/5dfd6d3f8ce44a36900bcac76f5b031f.png",
      "caption": ""
